{
  "ConnectionStrings": {
    "DefaultConnection": "",
    "AuditLogConnectionString": ""
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "BlockWhenFull": true,
        "configure": [
          {
            "Name": "Console",
            "Args": {
              "outputTemplate": "{UtcTimestamp:yyyy-MM-ddTHH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "Debug",
            "Args": {
              "outputTemplate": "{UtcTimestamp:yyyy-MM-ddTHH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "RollingFile",
            "Args": {
              "pathFormat": "/mnt/filedropshare/{Date}-MapSftpLog.txt",
              "outputTemplate": "{UtcTimestamp:yyyy-MM-ddTHH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          }
        ]
      }
    }
  },
  "Storage": {
    "FileDropRoot": "/mnt/filedropshare"
  },
  "PasswordExpirationDays": "60", // should match MAP setting
  "SmtpFromAddress": "map.support@milliman.com",
  "SmtpFromName": "Milliman Access Portal Support"
  //"SftpPasswordExpirationDays": "60"
  //"SftpMaintenanceIntervalMs": 20000
  //"SftpServerPrivateKey": "-----BEGIN RSA PRIVATE KEY-----\n blah blah blah \n-----END RSA PRIVATE KEY-----\n"
}
