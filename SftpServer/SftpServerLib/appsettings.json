{
  "ConnectionStrings": {
    "DefaultConnection": "",
    "AuditLogConnectionString": ""
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "BlockWhenFull": true,
        "configure": [
          {
            "Name": "Console",
            "Args": {
              "outputTemplate": "{UtcTimestamp:yyyy-MM-ddTHH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "Debug",
            "Args": {
              "outputTemplate": "{UtcTimestamp:yyyy-MM-ddTHH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "RollingFile",
            "Args": {
              "pathFormat": "C:\\MapSftpApplicationLog\\{Date}-MapSftpLog.txt",
              "outputTemplate": "{UtcTimestamp:yyyy-MM-ddTHH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          }
        ]
      }
    }
  },
  "Storage": {
    //"FileDropRoot": "\\\\indy-srv-02\\prm_test\\FileDropRoot"
  },
  "PasswordExpirationDays": "60",
  "SftpServerPrivateKey": "-----BEGIN RSA PRIVATE KEY-----\n blah blah blah \n-----END RSA PRIVATE KEY-----\n"
}
