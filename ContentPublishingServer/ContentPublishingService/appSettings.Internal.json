{
  "ConnectionStrings": {
  },
  "Storage": {
    "ContentItemRootPath": "D:\\MAP\\ContentItemRoot",
    "QvSourceDocumentsPath": "D:\\MAP\\QvSourceDocuments",
    "MapPublishingServerExchangePath": "D:\\MAP\\MapPublishingServerExchange"
  },
  "ServiceLaunchDelaySec": 0,
  "TraceLogDirectory": "D:\\MAP Publishing Service\\Logs",
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "BlockWhenFull": true,
        "configure": [
          {
            "Name": "Console",
            "Args": {
              "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "File",
            "Args": {
              "path": "D:\\MAP Publishing Service\\Logs\\ContentPublishingSvc.txt",
              "retainedFileCountLimit": 60,
              "rollingInteval": "Day",
              "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          }
        ]
      }
    }
  }
}
