        "BlockWhenFull": true,
        "configure": [
          {
            "Name": "Console",
            "Args": {
              "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "Debug",
            "Args": {
              "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "File",
            "Args": {
              "path": "C:\\MapPublishingServiceApplicationLog\\ContentPublishingSvc.txt",
              "retainedFileCountLimit": 60,
              "rollingInteval": "Day",
              "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          } //,
          //{
          //  "Name": "ApplicationInsights",
