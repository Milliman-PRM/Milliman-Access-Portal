{
  "Storage": {
    "ContentItemRootPath": "",
    "QvSourceDocumentsPath": "",
    "MapPublishingServerExchangePath": ""
  },

  "QvServerHost": "",
  "QvsQmsApiUrl": "",
  "QdsQmsApiUrl": "",
  "QvServerAdminUserAuthenticationDomain": "",
  "QvServerAdminUserName": "",
  "QvServerAdminUserPassword": "",

  "DefaultQdsTaskTimeoutMinutes": 10,
  "TaskAgeBeforeExecutionSeconds": 20,
  "StopWaitTimeSeconds": 180,
  "MaxParallelTasks": 1,
  "MaxSimultaneousRequests": 1,
  "ServiceLaunchDelaySec": 0,

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "BlockWhenFull": true,
        "configure": [
          {
            "Name": "Console",
            "Args": {
              "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "Debug",
            "Args": {
              "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          },
          {
            "Name": "File",
            "Args": {
              "path": "\\\\#{FileShareBaseUrl}\\logs\\ContentPublishingService\\ContentPublishingSvc.txt",
              "retainedFileCountLimit": 60,
              "rollingInteval": "Day",
              "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            }
          }
        ]
      }
    }
  }
}
