    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.4"
  },
  "scripts": {
    "build-dev": "webpack --config=webpack.dev.js",
    "build-prod": "webpack --config=webpack.prod.js",
    "test": "jest"
  },
  "jest-junit": {
    "output": "../../_test_results/jest-test-results.xml"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/ts/status-monitor.ts",
      "src/ts/shared.ts",
      "src/ts/react/**/*.{ts,tsx}",
      "src/ts/upload/**/*.{ts,tsx}",
      "!src/ts/upload/upload.ts",
      "src/ts/view-models/**/*.{ts,tsx}",
      "src/ts/content-access-admin/selection-status-monitor.ts",
      "src/ts/content-publishing/publication-status-monitor.ts"
    ],
    "coverageDirectory": "../../_test_results/_jest_coverage",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/src/ts/__mocks__/styleMock.ts"
    }
  },
  "-vs-binding": {
    "AfterBuild": [
      "install"
    ]
  },
  "engines": {
    "node": "8.9.4",
    "yarn": "1.12.3"
  }
}
