    "typescript": "^2.7.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-hot-middleware": "^2.21.2"
  },
  "scripts": {
    "build": "tsc & webpack",
    "test": "jest",
    "test-browsers": "testcafe chrome,firefox,ie src/browser_tests/"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/browser_tests/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/src/ts/__mocks__/styleMock.ts"
    }
  },
  "-vs-binding": {
    "AfterBuild": [
      "install"
    ]
  },
  "engines": {
    "node": "8.9.4"
  }
}
