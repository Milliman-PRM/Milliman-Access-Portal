{
  "Storage": {
    "FileUploadPath": "",
    "ContentItemRootPath": "",
    "MapPublishingServerExchangePath": "",
    "QvSourceDocumentsPath": ""
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "BlockWhenFull": true,
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "{UtcTimestamp:HH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "\\\\#{FileShareBaseUrl}\\logs\\MapApplicationLog\\MapLog.txt",
                "retainedFileCountLimit": 60,
                "rollingInterval": "Day",
                "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ]
  },
  "SftpServerHost": "sftp.map.milliman.com",
  "PbiAzureADClientID": "",
  "PbiAzureADUsername": "",
  "PbiTenantId": "",
  "TwoFactorEmailTokenLifetimeMinutes": 15,
  "MapRootUrl": "",
  "Global": {
    "ProhibitedDomains": [
      "aol.com",
      "gmail.com",
      "hotmail.com",
      "yahoo.com"
    ]
  },

  "ContainerRegistryUrl": "tbd",
  "ContainerRegistryCredential": "tbd",
  "ContainerRegistryScope": "tbd",

  "ContainerProxyScheme": "https",
  "ContainerProxyDomain": "map.milliman.com",
  "ContainerProxyPort": -1
}
