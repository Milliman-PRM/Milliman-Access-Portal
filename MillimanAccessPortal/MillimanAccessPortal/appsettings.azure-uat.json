{
  "Storage": {
    "FileUploadPath": "",
    "ContentItemRootPath": "",
    "MapPublishingServerExchangePath": "",
    "QvSourceDocumentsPath": ""
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "BlockWhenFull": true,
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "{UtcTimestamp:HH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "\\\\#{FileShareBaseUrl}\\logs\\MapApplicationLog\\MapLog.txt",
                "retainedFileCountLimit": 60,
                "rollingInterval": "Day",
                "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ]
  },
  "SftpServerHost": "sftp.map.milliman.com",
  "PbiAzureADClientID": "",
  "PbiAzureADUsername": "",
  "PbiTenantId": "",
  "TwoFactorEmailTokenLifetimeMinutes": 5,
  "MapRootUrl": "",

  "ContainerRegistryUrl": "",
  "ContainerRegistryUsername": "",
  "ContainerRegistryPassword": "",

  "AciResourceGroupName": "",
  "AciClientId": "",
  "AciClientSecret": "",
  "AciTenantId": "",
  "AciSubscriptionId": "",
  // "AciScope": "https://management.azure.com/.default",
  "AciGrantType": "client_credentials",

  "ContainerContentIpAddressType": "Private", // or "Private" for running in VNET
  "ContainerContentVnetId": "", // required for Private IpAddressType
  "ContainerContentVnetName": "" // required for Private IpAddressType
}
