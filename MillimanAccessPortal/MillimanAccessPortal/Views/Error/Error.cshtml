@model MillimanAccessPortal.Models.ErrorViewModels.GeneralErrorViewModel

@{
    ViewData["Title"] = "Application Error";
}

<div>
    <h2>Application Error</h2>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Message)
        </dt>
        <dd>
            @foreach (var item in Model.Message)
            {
                @(item)<br />
            }
        </dd>
    </dl>
</div>
<div>
    <a asp-controller=@Model.ReturnToController asp-action=@Model.ReturnToAction>Continue</a>
</div>
