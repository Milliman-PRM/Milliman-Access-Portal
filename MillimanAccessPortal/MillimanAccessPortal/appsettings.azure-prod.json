{
  "Storage": {
    "FileUploadPath": "",
    "ContentItemRootPath": "",
    "MapPublishingServerExchangePath": "",
    "QvSourceDocumentsPath": ""
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "BlockWhenFull": true,
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "{UtcTimestamp:HH:mm:ss.fffK} [{Level:u3}] {Message}{NewLine}{Exception}"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "\\\\#{FileShareBaseUrl}\\logs\\MapApplicationLog\\MapLog.txt",
                "retainedFileCountLimit": 60,
                "rollingInterval": "Day",
                "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ]
  },
  "SftpServerHost": "sftp.map.milliman.com",
  "PbiAzureADClientID": "",
  "PbiAzureADUsername": "",
  "PbiTenantId": "",
  "TwoFactorEmailTokenLifetimeMinutes": 15,
  "MapRootUrl": "",
  "Global": {
    "ProhibitedDomains": [
      "hotmail.com",
      "gmail.com",
      "aol.com"
    ]
  },

  "ContainerRegistryUrl": "",
  "ContainerRegistryUsername": "",
  "ContainerRegistryPassword": "",

  "AciResourceGroupName": "",
  "AciClientId": "",
  "AciClientSecret": "",
  "AciTenantId": "",
  "AciSubscriptionId": "",
  "AciGrantType": "client_credentials",

  "ContainerContentIpAddressType": "Private", // "Private" or "Public"
  "ContainerContentVnetId": "", // Value is required for Private IpAddressType
  "ContainerContentVnetName": "" // Value is required for Private IpAddressType
}
