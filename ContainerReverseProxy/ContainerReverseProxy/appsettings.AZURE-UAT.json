{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MapHubUrl": "https://map-uat-app.trafficmanager.net/contentsessionhub",
  "ReverseProxyBaseUrl": "http://localhost:80",
  "ReverseProxyPathBaseSegment": "ContentProxy",
  "ReverseProxyContentTokenHeaderName": "content-token",
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "{UtcTimestamp:HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
              }
            },
            {
              "Name": "Debug",
              "Args": {
                "outputTemplate": "{UtcTimestamp:HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "/mnt/proxylogs/reverseproxylog.txt",
                "retainedFileCountLimit": 60,
                "rollingInterval": "Day",
                "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ]
  }
}
