{
  "ReverseProxyBaseUrl": "https://localhost:7016",
  "ReverseProxyContentTokenHeaderName": "content-token",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MapHubUrl": "https://localhost:44336/contentsessionhub",
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "outputTemplate": "{UtcTimestamp:HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
              }
            },
            {
              "Name": "Debug",
              "Args": {
                "outputTemplate": "{UtcTimestamp:HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
              }
            }
            //{
            // "Name": "File",
            // "Args": {
            // "path": "C:\\MapApplicationLog\\MapLog.txt",
            // "retainedFileCountLimit": 60,
            // "rollingInterval": "Day",
            // "outputTemplate": "{UtcTimestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
            // }
            //}
          ]
        }
      }
    ]
  }
}